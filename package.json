{
  "name": "shredly2",
  "version": "2.0.0",
  "description": "Personalized workout generation engine",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "cli": "tsx cli/test-runner.ts",
    "cli:interactive": "tsx cli/interactive-questionnaire.ts",
    "cli:edit": "tsx cli/edit-workout.ts",
    "cli:watch": "nodemon --exec tsx cli/test-runner.ts",
    "typecheck": "svelte-kit sync && tsc --noEmit",
    "lint": "eslint . --ext .ts,.js,.svelte",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:diagnostics": "tsx tests/diagnostics/exercise-filtering-diagnostics.ts"
  },
  "keywords": [
    "fitness",
    "workout",
    "training",
    "exercise"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@capacitor/cli": "^8.0.0",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.49.4",
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vite-pwa/sveltekit": "^1.1.0",
    "@vitest/ui": "^4.0.16",
    "autoprefixer": "^10.4.23",
    "eslint": "^8.55.0",
    "happy-dom": "^20.0.11",
    "inquirer": "^13.1.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.5.6",
    "svelte": "^5.46.1",
    "tailwindcss": "^3.4.19",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@capacitor/core": "^8.0.0",
    "chalk": "^5.6.2"
  }
