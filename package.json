{
  "name": "shredly2",
  "version": "2.0.0",
  "description": "Personalized workout generation engine",
  "type": "module",
  "scripts": {
    "cli": "tsx cli/test-runner.ts",
    "cli:interactive": "tsx cli/interactive-questionnaire.ts",
    "cli:edit": "tsx cli/edit-workout.ts",
    "cli:watch": "nodemon --exec tsx cli/test-runner.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.js",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:diagnostics": "tsx tests/diagnostics/exercise-filtering-diagnostics.ts"
  },
  "keywords": [
    "fitness",
    "workout",
    "training",
    "exercise"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitest/ui": "^4.0.16",
    "eslint": "^8.55.0",
    "happy-dom": "^20.0.11",
    "inquirer": "^13.1.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "chalk": "^5.6.2"
  }
}
