  "split_muscle_group_mapping": {
    "description": "Maps base split focus types to muscle groups for exercise selection filtering. Suffixed focus types (e.g., Upper-HIIT, Push-Volume) use getBaseFocus() to look up their base type mapping. Suffixes only affect day structure (compound-only vs strength blocks), not muscle group selection.",
    "Full Body": {
      "include_muscle_groups": ["all"],
      "description": "No filtering - select from all muscle groups"
    },
    "Upper": {
      "include_muscle_groups": ["Chest", "Back", "Shoulders", "Biceps", "Triceps", "Core", "Lats", "Rhomboids", "Traps", "Forearms"],
      "exclude_muscle_groups": ["Quadriceps", "Hamstrings", "Glutes", "Calves", "Adductors"],
      "description": "Upper body muscle groups only"
    },
    "Lower": {
      "include_muscle_groups": ["Quadriceps", "Hamstrings", "Glutes", "Calves", "Adductors", "Core"],
      "exclude_muscle_groups": ["Chest", "Back", "Shoulders", "Biceps", "Triceps", "Lats", "Rhomboids", "Traps", "Forearms"],
      "description": "Lower body muscle groups only"
    },
    "Push": {
      "include_muscle_groups": ["Chest", "Shoulders", "Triceps", "Front Delts", "Side Delts", "Arms", "Upper Chest"],
      "exclude_muscle_groups": ["Back", "Biceps", "Lats", "Rhomboids", "Traps", "Rear Delts"],
      "description": "Pushing muscles - chest, shoulders, triceps, arms (Core removed, legs allowed as secondary)"
    },
    "Pull": {
      "include_muscle_groups": ["Back", "Biceps", "Lats", "Rhomboids", "Traps", "Rear Delts", "Forearms", "Upper Back", "Lower Back"],
      "exclude_muscle_groups": ["Chest", "Shoulders", "Front Delts", "Side Delts", "Triceps"],
      "description": "Pulling muscles - back, biceps, rear delts (Core removed, legs allowed as secondary)"
    },
    "Legs": {
      "include_muscle_groups": ["Quadriceps", "Hamstrings", "Glutes", "Calves", "Adductors", "Core"],
      "exclude_muscle_groups": ["Chest", "Back", "Shoulders", "Biceps", "Triceps", "Lats", "Rhomboids", "Traps", "Forearms"],
      "description": "All leg muscle groups"
    },
    "Mobility": {
      "include_muscle_groups": ["all"],
      "description": "Mobility/flexibility/recovery day - used for Mobility, FullBody-Mobility, and Flexibility focus types"
    }
  },

